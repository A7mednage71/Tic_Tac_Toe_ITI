<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox spacing="10" alignment="CENTER"
      xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.mycompany.finalprojectclient.AuthController">

    <TextField fx:id="txtUser" promptText="Username"/>
    <TextField fx:id="txtEmail" promptText="Email"/>
    <PasswordField fx:id="txtPass" promptText="Password"/>

    <Button text="Register" onAction="#register"/>
    <Button text="Back to Login" onAction="#goLogin"/>
</VBox>
